Package: GAClustR
Type: Package
Title: Genetic Algorithms for Enhancing Clustering of Gene Expression
        Data
Version: 0.99.1
Authors@R: 
    person(given = "Zejia", family = "Chen", email = "zchen@qmed.ca", role = c("aut", "cre"),
